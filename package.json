{
  "name": "tsboilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "tsc": "tsc --noemit",
    "tsc:watch": "tsc --noemit --watch",
    "tslint": "tslint --project . -t stylish",
    "tslint:watch": "nodemon -e ts,tsx -w src -w tslint.json -d 0.2 -w tsconfig.json -- node_modules/.bin/tslint --project . -t stylish",
    "clearwatchman": "watchman watch-del-all && rm -rf node_modules/ && yarn cache clean && yarn && lsof -ti: 8081 | xargs kill && react-native start",
    "postinstall": "cd ios && pod install"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "es6-symbol": "^3.1.3",
    "jsc-android": "241213.1.0",
    "react": "16.13.1",
    "react-native": "0.62.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-syntax-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime": "^7.11.2",
    "@react-native-community/cli": "^4.13.0",
    "@types/axios-mock-adapter": "^1.10.0",
    "@types/enzyme": "^3.10.7",
    "@types/node": "^14.11.2",
    "@types/jest": "^26.0.14",
    "@types/react": "^16.9.49",
    "@types/react-native": "^0.63.22",
    "@welldone-software/why-did-you-render": "^4.3.2",
    "axios-mock-adapter": "^1.18.2",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-preset-react-native": "^4.0.1",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "form-data": "^3.0.0",
    "husky": "^4.3.0",
    "jest": "25.5.4",
    "jest-enzyme": "^7.1.2",
    "jetifier": "^1.6.6",
    "jsdom": "^16.2.2",
    "lint-staged": "^10.4.0",
    "metro-react-native-babel-preset": "^0.63.0",
    "nodemon": "^2.0.4",
    "react-dom": "^16.13.1",
    "react-native-rename": "^2.4.1",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "16.13.1",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "^0.13.7",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.1",
    "tslint": "^5.20.1",
    "tslint-clean-code": "^0.2.10",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-react": "^4.2.0",
    "tslint-react-native": "^0.0.7",
    "typescript": "^3.9.7"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn tslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn run tsc"
    }
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native-community|react-navigation|rn-actionsheet-module)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>jest-setup.js"
    ],
    "globals": {
      "window": {}
    },
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**"
    ],
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testRegex": ".*\\.test.tsx?",
    "testEnvironment": "jsdom"
  },
  "volta": {
    "node": "10.16.3",
    "yarn": "1.17.3"
  }
}
